{
  "name": "@es-vanguard/telemetry",
  "type": "module",
  "scripts": {
    "compile": "tsc --project tsconfig.build.json",
    "type-check": "tsc --noEmit",
    "try": "bun run tests/try.ts"
  },
  "dependencies": {
    "@es-vanguard/utils": "workspace:*",
    "serialize-error": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@es-vanguard/typescript-config": "workspace:*",
    "@types/bun": "catalog:types",
    "typescript": "catalog:"
  },
  "exports": {
    "./errors": {
      "default": "./dist/src/errors/classes.js",
      "types": "./dist/src/errors/classes.d.ts"
    },
    "./errors/fallback": {
      "default": "./dist/src/errors/fallback.js",
      "types": "./dist/src/errors/fallback.d.ts"
    },
    "./context": {
      "default": "./dist/src/types/context.js",
      "types": "./dist/src/types/context.d.ts"
    }
  }
